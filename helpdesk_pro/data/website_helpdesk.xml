        <record id="waiting_ticket_request_email_template" model="mail.template">
            <field name="name">Ticket: Waiting actions</field>
            <field name="model_id" ref="helpdesk_pro.model_helpdesk_ticket"/>
            <field name="subject">Waiting actions</field>
            <field name="email_from">{{ (object.user_id.email_formatted or user.email_formatted) }}</field>
            <field name="email_to">{{ (object.partner_email if not object.sudo().partner_id.email or object.sudo().partner_id.email != object.partner_email else '') }}</field>
            <field name="body_html" type="xml">
                <div style="margin: 0px; padding: 0px;">
                    <p style="margin: 0px; padding: 0px;">
                        This automatic message informs you that the technician in charge of your ticket (reference <t t-out="object.id or ''" contenteditable="false" data-oe-t-inline="true">15</t>) is waiting for some action from you.<br/>
                        Access your personal area of our portal and check what is required by the technician.<br/>
                        If you have any further questions or comments, do not hesitate to contact us through your personal area<br/><br/>
                        Thank you for your cooperation.<br/>
                        Kind regards,<br/><br/>
                        <t t-out="object.team_id.name or 'Helpdesk'" contenteditable="false" data-oe-t-inline="true">Helpdesk</t> Team.
                    </p>
                </div>
            </field>
            <field name="lang">{{ object.partner_id.lang or object.user_id.lang or user.lang }}</field>
            <field name="auto_delete" eval="False"/>
        </record>

        <record id="process_ticket_request_email_template" model="mail.template">
            <field name="name">Ticket: In Progress</field>
            <field name="model_id" ref="helpdesk_pro.model_helpdesk_ticket"/>
            <field name="subject">In Progress</field>
            <field name="email_from">{{ (object.user_id.email_formatted or user.email_formatted) }}</field>
            <field name="email_to">{{ (object.user_id.email_formatted or user.email_formatted) }}</field>
            <field name="body_html" type="html">
                <div>
                    Dear <t t-out="object.user_id.name or ''"></t>,<br/><br/>
                    The ticket
                    <t t-if="object.get_portal_url()" data-oe-t-group="0" data-oe-t-group-active="true">
                        <a t-attf-href="/my/ticket/{{ object.id }}/{{ object.access_token }}" t-out="object.name or ''" contenteditable="false"></a>
                    </t>
                    to which it has been assigned is operational on the Support platform.<br/><br/>
                    Kind regards,<br/><br/>
                    <t t-out="object.team_id.name or 'Helpdesk'" contenteditable="false" data-oe-t-inline="true">Helpdesk</t>.
                </div>
            </field>
            <field name="lang">{{ object.partner_id.lang or object.user_id.lang or user.lang }}</field>
            <field name="auto_delete" eval="False"/>
        </record>

        <!-- Sequence -->
        <record id="helpdesk_ticket_sequence" model="ir.sequence">
            <field name="name">Helpdesk Ticket Sequence</field>
            <field name="code">helpdesk.ticket.sequence</field>
            <field name="prefix">TICK</field>
            <field name="padding">5</field>
            <field name="company_id" />
        </record>
        <!-- Stages -->

        <record id="helpdesk_ticket_stage_new" model="helpdesk.ticket.stage">
            <field name="sequence">1</field>
            <field name="name">New</field>
            <field name="unattended">True</field>
            <field name="closed">False</field>
            <field name="company_id"/>
            <field name="key_stage">new</field>
        </record>

        <record id="helpdesk_ticket_stage_in_progress" model="helpdesk.ticket.stage">
            <field name="sequence">2</field>
            <field name="name">In Progress</field>
            <field name="unattended">False</field>
            <field name="closed">False</field>
            <field name="company_id"/>
            <field name="key_stage">pro</field>
            <field name="mail_template_id" ref="helpdesk_pro.process_ticket_request_email_template"/>
        </record>
        <record id="helpdesk_ticket_stage_awaiting" model="helpdesk.ticket.stage">
            <field name="sequence">3</field>
            <field name="name">Waiting</field>
            <field name="unattended">False</field>
            <field name="closed">False</field>
            <field name="company_id"/>
            <field name="key_stage">wai</field>
            <field name="mail_template_id" ref="helpdesk_pro.waiting_ticket_request_email_template"/>
        </record>
        <record id="helpdesk_ticket_stage_done" model="helpdesk.ticket.stage">
            <field name="sequence">4</field>
            <field name="name">Done</field>
            <field name="unattended">False</field>
            <field name="closed">True</field>
            <field name="fold">True</field>
            <field name="key_stage">don</field>
            <field name="mail_template_id" ref="helpdesk_pro.closed_ticket_template" />
            <field name="company_id"/>
        </record>
        <record id="helpdesk_ticket_stage_cancelled" model="helpdesk.ticket.stage">
            <field name="sequence">5</field>
            <field name="name">Cancelled</field>
            <field name="unattended">False</field>
            <field name="closed">True</field>
            <field name="fold">True</field>
            <field name="key_stage">can</field>
            <field name="mail_template_id" ref="helpdesk_pro.closed_ticket_template" />
            <field name="company_id"/>
        </record>

