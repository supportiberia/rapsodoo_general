        <!--Email template Notification In progress -->
        <record id="notification_email_progress_ticket_template" model="mail.template">
            <field name="name">Helpdesk Pro: Send Notification</field>
            <field name="model_id" ref="helpdesk_pro.model_helpdesk_ticket"/>
            <field name="subject">Ticket {{object.number}} in "In progress"</field>
            <field name="body_html" type="xml">
                <div>
                    Dear <t t-out="object.user_id.partner_id.name or ''">User</t>.<br/>
                    The ticket "<t t-out="object.name or ''">TICKET </t>" with reference (<t t-out="object.number or ''"># </t>) takes some days in 'In Progress'.<br/><br/>
                    Kind regards,<br/><br/>
                    <t t-out="object.team_id.name or 'Department'"> Team</t>.
                </div>
            </field>
            <field name="lang">{{ object.user_id.lang or user.lang }}</field>
            <field name="auto_delete" eval="False"/>
        </record>
