#. module: helpdesk_pro
#: model:mail.template,body_html:helpdesk_pro.closed_ticket_template
msgid ""
"<?xml version=\"1.0\"?>\n"
"<div>\n"
"                    Hello {{object.partner_id.name}}.<br/>\n"
"                    Ticket (reference <a t-attf-href=\"/my/ticket/{{object.id}}\"><t t-out=\"object.number or ''\"># </t></a>) has been closed.<br/><br/>\n"
"                    If you have any further questions or comments, do not hesitate to contact us through your personal area<br/><br/>\n"
"                    Kind regards,<br/><br/>\n"
"                    <t t-out=\"object.team_id.name or 'Support'\"> Team</t>.\n"
"                </div>\n"
"            "
msgstr ""
"<?xml version=\"1.0\"?>\n"
"<div>\n"
"                    Hola {{object.partner_id.name}}.<br/>\n"
"                    El ticket (con referencia <a t-attf-href=\"/my/ticket/{{object.id}}\"><t t-out=\"object.number or ''\"># </t></a>) ha sido cerrado.<br/><br/>\n"
"                    Si tienes más dudas o comentarios, no dudes en ponerte en contacto con nosotros a través de tu área personal<br/><br/>\n"
"                    Saludos cordiales,<br/><br/>\n"
"                    <t t-out=\"object.team_id.name or 'Support'\"> Equipo</t>.\n"
"                </div>\n"
"            "

#. module: helpdesk_pro
#: model:mail.template,body_html:helpdesk_pro.waiting_ticket_request_email_template
msgid ""
"<?xml version=\"1.0\"?>\n"
"<div>\n"
"                    This automatic message informs you that the technician in charge of your ticket (reference <a t-attf-href=\"/my/ticket/{{object.id}}\"><t t-out=\"object.number or ''\"># </t></a>) is waiting for some action from you.<br/>\n"
"                    Access your personal area of our portal and check what is required by the technician.<br/>\n"
"                    If you have any further questions or comments, do not hesitate to contact us through your personal area<br/><br/>\n"
"                    Thank you for your cooperation.<br/>\n"
"                    Kind regards,<br/><br/>\n"
"                    <t t-out=\"object.team_id.name or 'Support'\"> Team</t>.\n"
"                </div>\n"
"            "
msgstr ""
"<?xml version=\"1.0\"?>\n"
"<div>\n"
"                    Este mensaje automático le informa que el técnico a cargo de su boleto (referencia <a t-attf-href=\"/my/ticket/{{object.id}}\"><t t-out=\"object.number or ''\"># </t></a>) está esperando alguna acción de usted.<br/>\n"
"                    Accede a tu área personal de nuestro portal y consulta lo que requiere el técnico.<br/>\n"
"                    Si tienes más dudas o comentarios, no dudes en ponerte en contacto con nosotros a través de tu área personal<br/><br/>\n"
"                    Gracias por su cooperación.<br/>\n"
"                    Saludos cordiales,<br/><br/>\n"
"                    <t t-out=\"object.team_id.name or 'Support'\"> Equipo</t>.\n"
"                </div>\n"
"            "

